[flake8]
max-line-length=120
max-local-variables=8
max-complexity=8
max-module-members=10
max-line-complexity=18

extend-ignore=
        # encoding
        C101,
        # black brother
        BLK100,
per-file-ignores=
        ./alembic/versions/*:
            W291
            E128
            E501
        settings.py:
            E501
        __init__.py:
            F401
